
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/bg"
    tools:context=".DartboardActivity" >

    <RelativeLayout
        android:id="@+id/game_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttons"
        android:layout_alignParentTop="true"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin" >

        <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/button5"
                android:layout_marginTop="10dp"
                android:text="رقم اللوحة"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="@color/FontColorLogin"
                android:textSize="20sp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/textView1"
            android:layout_marginTop="10dp"
            android:text="اسم المستخدم"
            android:textColor="@color/FontColorLogin"
            android:textSize="20sp" />



        <TextView
            android:id="@+id/TextView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/TextView01"
            android:layout_marginTop="10dp"
            android:text="إسم السائق"
            android:textColor="@color/FontColorLogin"
            android:textSize="20sp" />


        <TextView
                android:id="@+id/TextView33"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/TextView3"
                android:layout_marginTop="10dp"
                android:text="رقم الجوال"
                android:textColor="@color/FontColorLogin"
                android:textSize="20sp" />

        <TextView
                android:id="@+id/TextView34"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/TextView33"
                android:layout_marginTop="10dp"
                android:text="عدد الطالبات "
                android:textColor="@color/FontColorLogin"
                android:textSize="20sp" />

        <TextView
                android:id="@+id/TextView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/textView2"
                android:layout_marginTop="10dp"
                android:text="إسم المدرسة"
                android:textColor="@color/FontColorLogin"
                android:textSize="20sp" />

        <TextView
                android:id="@+id/TextView01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/TextView4"
                android:layout_marginTop="10dp"
                android:text="العنوان"
                android:textColor="@color/FontColorLogin"
                android:textSize="20sp" />


        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView1"
            android:layout_alignBottom="@+id/textView1"
            android:layout_alignRight="@+id/editText1"
            android:gravity="right"
            android:text=""
            android:textColor="@color/FontColorLogin"
            android:textSize="20sp" />


        <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/textView2"
                android:layout_alignRight="@+id/editText1"
                android:gravity="right"
                android:text=""
                android:textColor="@color/FontColorLogin"
                android:textSize="20sp" />






        <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/TextView4"
                android:layout_alignBottom="@+id/textView10"
                android:layout_alignRight="@+id/editText1"
                android:gravity="right"
                android:text=""
                android:textColor="@color/FontColorLogin"
                android:textSize="20sp" />


        <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/TextView01"
                android:layout_alignBottom="@+id/textView5"
                android:layout_alignRight="@+id/editText1"
                android:gravity="right"
                android:text=""
                android:textColor="@color/FontColorLogin"
                android:textSize="20sp" />

        <EditText
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_alignBottom="@+id/TextView3"
                android:layout_alignRight="@+id/editText1"
                android:gravity="right"
                android:text=""
                android:ems="30"
                android:hint="أسم السائق"
                android:textColor="@color/FontColorLogin"
                android:textSize="20sp" />

        <EditText
                android:id="@+id/textView44"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_alignBottom="@+id/TextView33"
                android:layout_alignRight="@+id/editText1"
                android:gravity="right"
                android:inputType="number"
                android:hint="رقم الجوال"
                android:ems="30"
                android:maxLength="10"
                android:textColor="@color/FontColorLogin"
                android:textSize="20sp" />

        <EditText
                android:id="@+id/editText33"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_alignRight="@+id/editText1"
                android:layout_alignBottom="@+id/TextView34"
                android:ems="30"
                android:gravity="right"
                android:maxLength="2"
                android:hint="عدد الطالبات "
                android:inputType="number"
                android:textColor="@color/FontColorLogin"
                android:textCursorDrawable="@null"
                android:textSize="20sp" />

        <TextView
                android:id="@+id/TextView02"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/editText2"
                android:layout_alignRight="@+id/textView1"
                android:layout_alignTop="@+id/editText2"
                android:text="رقم الوزارة"
                android:textColor="@color/FontColorLogin"
                android:textSize="20sp" />


        <EditText
                android:id="@+id/editText2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_toLeftOf="@+id/textView2"
                android:ems="8"
                android:hint="رقم الوزارة"
                android:inputType="number"
                android:textColor="@color/FontColorLogin"
                android:textCursorDrawable="@null"
                android:textSize="20sp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/button1"
            android:layout_alignBottom="@+id/button1"
            android:layout_alignRight="@+id/textView1"
            android:text="رقم الشركه"
            android:textColor="@color/FontColorLogin"
            android:textSize="20sp" />

        <TextView
                android:id="@+id/textView61"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/editText3"
                android:layout_alignRight="@+id/textView1"
                android:text="رقم الهوية"
                android:textColor="@color/FontColorLogin"
                android:textSize="20sp" />









        <EditText
            android:id="@+id/editText1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/editText2"
            android:layout_below="@+id/button01"
            android:ems="8"
            android:hint="رقم الشركة"
            android:inputType="number"
            android:textColor="@color/FontColorLogin"
            android:textCursorDrawable="@null"
            android:textSize="20sp" />

        <EditText
                android:id="@+id/editText3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/editText2"
                android:layout_below="@+id/editText1"
                android:ems="8"
                android:maxLength="10"
                android:hint="رقم الهوية"
                android:inputType="number"
                android:textColor="@color/FontColorLogin"
                android:textCursorDrawable="@null"
                android:textSize="20sp" />



        <Button
            android:id="@+id/button5"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/TextView02"
            android:layout_below="@+id/editText3"
            android:background="@drawable/line" />

        <Button
            android:id="@+id/button01"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_alignBaseline="@+id/editText2"
            android:layout_alignBottom="@+id/editText2"
            android:layout_toLeftOf="@+id/editText2"
            android:background="@drawable/search"
            android:textColor="@color/FontColorLogin" />

        <Button
            android:id="@+id/button1"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_alignBaseline="@+id/editText1"
            android:layout_alignBottom="@+id/editText1"
            android:layout_toLeftOf="@+id/editText1"
            android:background="@drawable/search"
            android:textColor="@color/FontColorLogin" />


        <Button
                android:id="@+id/button02"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_alignBaseline="@+id/editText3"
                android:layout_alignBottom="@+id/button1"
                android:layout_toLeftOf="@+id/editText3"
                android:background="@drawable/search"
                android:textColor="@color/FontColorLogin" />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/buttons"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:background="#333"
        android:orientation="horizontal" >

        
        <Button
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="200dp"
            android:background="@drawable/sync" />

        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/save" /> 
                    
    </LinearLayout>

</RelativeLayout>